	<body>

		<!-- Page Wrapper -->
			<div id="page-wrapper">

				<!-- Header -->
					<header id="header">
						<h1><a href="/">TimePlayed</a></h1>
						<% include('../partials/nav') %>
					</header>

				<!-- Main -->
					<article id="main">
						<header>
							<h2>No access!</h2>
						</header>
						<section class="wrapper style5" style="padding-top: 3em;">
							<div class="inner">
							<section style="position: relative">
								Whoops! You don't have access to this server!<br>
								<% if(privacy == 1) { %>
									<% if(userInfo) { %>
										This server's profile is only visible to members of this server!
									<% } else { %>
										This server's profile is only visible to members of this server! If you are a member of this server, please <a onclick="login()" href="#">login</a> and try again.
									<% } %>
								<% } else { %>
									<% if(userInfo) { %>
										This server's profile is only visible to it's server members with the <code>Manage Server</code> permission. If you do have this permission, please <a onclick="login()" href="#">login</a> and try again.
									<% } else { %>
										This server's profile is only visible to it's server members with the <code>Manage Server</code> permission. If you do have this permission, please <a onclick="login()" href="#">login</a> and try again.
									<% } %>
								<% } %>
								<script>
									function login() {
										var url = encodeURI(window.location.pathname);
										window.location.href = "/login?redirect=" + url;
									}
								</script>
							</section>
							</div>
						</section>
					</article>

